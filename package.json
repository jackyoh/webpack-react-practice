{
  "name": "react-webpack-babel",
  "version": "0.0.2",
  "description": "React Webpack Babel Starter Kit",
  "main": "''",
  "scripts": {
    "dev": "yarn webpack-dev-server --config webpack.config.dev.js ",
    "build":
      "NODE_ENV=production webpack -p --config webpack.config.prod.js --progress --profile --colors",
    "webpack-dev-server": "webpack-dev-server"
  },
  "keywords": ["React", "Webpack", "Babel", "Starter", "template"],
  "license": "MIT",
  "dependencies": {
    "bootstrap": "3.3.5",
    "react": "15.6.2",
    "react-dom": "15.5.4"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "6.5.0",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.5",
    "style-loader": "0.13.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3"
  }
}
